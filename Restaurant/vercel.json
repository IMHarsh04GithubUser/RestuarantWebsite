{
  "version": 2,
  "builds": [
    {
      "src": "RestBackend\index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "frontend/build" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "RestBackend\index.js"
    },
    {
      "src": "/(.*)",
      "dest": "Restaurant\index.html"
    }
  ]
}
